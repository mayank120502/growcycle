msgctxt "Languages::sd_google_analytics.event.require_logs.name"
msgid "The GA configuration requires HTTP request logs enabled"
msgstr "The GA configuration requires HTTP request logs enabled"

msgctxt "Languages::sd_google_analytics.event.require_logs.message"
msgid "The logging for <b>HTTP/HTTPS requests</b> is not enabled. It is required to track and analyze the correctness of purchase/refund transactions sent to Google Analytics Enhanced Ecommerce so please enable it in <a href=\"[link]\" target=\"_blank\">the global settings</a>"
msgstr "The logging for <b>HTTP/HTTPS requests</b> is not enabled. It is required to track and analyze the correctness of purchase/refund transactions sent to Google Analytics Enhanced Ecommerce so please enable it in <a href=\"[link]\" target=\"_blank\">the global settings</a>"
