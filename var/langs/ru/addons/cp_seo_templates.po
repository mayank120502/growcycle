msgid "{{ '12345'|slice(1, 2) }} result: 23"
msgstr "{{ '12345'|slice(1, 2) }} результат: 23"

msgctxt "Languages::cp_seo_filter_reverse_descr"
msgid "Reverses a string"
msgstr "Переворачивает строку"

msgctxt "Languages::cp_seo_filter_trim_descr"
msgid "Strips whitespace"
msgstr "Удаляет пробелы"

msgctxt "Languages::cp_seo_filter_trim_tp"
msgid "Strips hitespace (or other characters) from the beginning and end of a string"
msgstr "Удаляет пробелы (или другие символы) из начала и конца строки"

msgctxt "Languages::cp_seo_filter_lower_descr"
msgid "Converts a value to lowercase"
msgstr "Преобразует значение в нижний регистр"

msgctxt "Languages::cp_seo_filter_upper_descr"
msgid "Converts a value to uppercase"
msgstr "Преобразует значение в верхний регистр"

msgctxt "Languages::cp_seo_filter_capitalize_descr"
msgid "Capitalizes a value"
msgstr "Возвращает значение в заглавном регистре"

msgctxt "Languages::cp_seo_filter_capitalize_tp"
msgid "The first character will be uppercase, all others lowercase"
msgstr "Первый символ будет в верхнем регистре, все остальные в нижнем регистре"

msgctxt "Languages::cp_seo_filter_title_descr"
msgid "Returns a titlecased version of the value"
msgstr "Возвращает значение в заглавном регистре"

msgctxt "Languages::cp_seo_filter_title_tp"
msgid "Words will start with uppercase letters, all remaining characters are lowercase"
msgstr "Слова будут начинаться с заглавных букв, все остальные символы в нижнем регистре"

msgctxt "Languages::cp_seo_filter_abs_descr"
msgid "Returns the absolute value"
msgstr "Возвращает абсолютное значение"

msgctxt "Languages::cp_seo_filter_round_descr"
msgid "Rounds a number to a given precision"
msgstr "Округляет число до заданной точности"

msgctxt "Languages::cp_seo_filter_round_tp"
msgid "The round filter takes two optional arguments; the first one specifies the precision (default is 0) and the second the rounding method (default is common)"
msgstr "Фильтр принимает два необязательных аргумента; первый указывает точность (по умолчанию 0), а второй - метод округления (по умолчанию используется common)"

