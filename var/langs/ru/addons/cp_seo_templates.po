msgctxt "Languages::cp_seo_filter_last_descr"
msgid "Returns the last symbol of a string"
msgstr "Возвращает последний символ строки"

msgctxt "Languages::cp_seo_filter_last_tp"
msgid "{{ '1234'|last }} result: 4"
msgstr "{{ '1234'|last }} результат: 4"

msgctxt "Languages::cp_seo_filter_first_descr"
msgid "Returns the first symbol of a string"
msgstr "Возвращает первый символ строки"

msgctxt "Languages::cp_seo_filter_first_tp"
msgid "{{ '1234'|first }} result: 1"
msgstr "{{ '1234'|first }} результат: 1"

msgctxt "Languages::cp_seo_filter_slice_descr"
msgid "Extracts a slice of a string"
msgstr "Извлекает кусок строки"

