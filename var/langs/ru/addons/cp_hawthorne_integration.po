msgctxt "Addons::name::cp_hawthorne_integration"
msgid "Cart-Power: Hawthorne Hydroponics integration"
msgstr "Cart-Power: Интеграция Hawthorne Hydroponics"

msgctxt "Addons::description::cp_hawthorne_integration"
msgid "This add-on allows to get the list of products from Hawthorne Hydroponics. Developed by <a target=\"_blank\" href=\"http://cart-power.com/\">Cart-Power</a>"
msgstr "Этот модуль позволяет получить список продуктов из Hawthorne Hydroponics. Разработано <a target=\"_blank\" href=\"http://cart-power.ru/\">Cart-Power</a>"

msgctxt "SettingsSections::cp_hawthorne_integration::general"
msgid "General"
msgstr "Общие"

msgctxt "SettingsOptions::cp_hawthorne_integration::client_id"
msgid "\"Key\" used for authorization at Hawthorne"
msgstr "\"Ключ\" используемый для авторизации на по API"

msgctxt "SettingsTooltips::cp_hawthorne_integration::client_id"
msgid "Also in some documentation it is called \"client_id\""
msgstr "Кое-где в документации он также называется \"client_id\""

msgctxt "SettingsOptions::cp_hawthorne_integration::client_secret"
msgid "\"Secret\" used for authorization at Hawthorne"
msgstr "\"Секрет\" используемый для авторизации на по API"

msgctxt "SettingsTooltips::cp_hawthorne_integration::client_secret"
msgid "Also in some documentation it is called \"client_secret\""
msgstr "Кое-где в документации он также называется \"client_secret\""

msgctxt "SettingsOptions::cp_hawthorne_integration::company_id"
msgid "Vendor that will own poducts of Hawthorne"
msgstr "Продавец, который будет владеть продуктами из Hawthorne"

msgctxt "Languages::cp_hawthorne_startup_info"
msgid "You can mange products import using spacial page at \"Vendors\" -> \"Hawthorne import\""
msgstr "Вы можете управлять импортом товаров, используя специальную страницу  \"Продавцы\" -> \"Hawthorne импорт\""

msgctxt "SettingsOptions::cp_hawthorne_integration::import_category_id"
msgid "Disabled category used for newly imported products"
msgstr "Отключенная категория, используемая для импорта новых товаров"

msgctxt "SettingsTooltips::cp_hawthorne_integration::import_category_id"
msgid "You can use only disabled root categories"
msgstr "Можно использовать только категории первого уровня"

msgctxt "SettingsOptions::cp_hawthorne_integration::cron_password"
msgid "Cron password"
msgstr "Пароль для крона"

msgctxt "SettingsTooltips::cp_hawthorne_integration::cron_password"
msgid "Use a password to restrict external access to CRON"
msgstr "Используйте пароль для ограничения доступа к CRON"

