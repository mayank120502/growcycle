msgctxt "Addons::name::cp_discussion"
msgid "Cart-Power: Random review date"
msgstr "Cart-Power: Случайная дата отзывов"

msgctxt "Addons::description::cp_discussion"
msgid "Generates a random date for reviews. Developed by <a target=\"_blank\" href=\"http://cart-power.com/\">Cart-Power</a>"
msgstr "Генерирует случайную дату для отзывов товаров и обсуждений. Разработано <a target=\"_blank\" href=\"http://cart-power.com/\">Cart-Power</a>"

msgctxt "SettingsOptions::cp_discussion::cp_discussion_header_settings"
msgid "Add a random creation time for all product reviews and discussions in the date range"
msgstr "Добавить случайное время создания для всех отзывов товаров и обсуждений в диапазоне дат"

msgctxt "Languages::cp_discussion.rnd_time_storefront"
msgid "Add random creation time when creating product reviews and discussions"
msgstr "Добавить случайное время создания при создании отзывов товаров и обсуждений"

msgctxt "Languages::cp_discussion.use_rnd_time"
msgid "Use random time"
msgstr "Используйте случайное время"

msgctxt "Languages::cp_discussion.am"
msgid "AM"
msgstr "AM"

msgctxt "Languages::cp_discussion.pm"
msgid "PM"
msgstr "PM"

msgctxt "Languages::cp_discussion.enter_time_more_current"
msgid "Field cannot be more than the current date"
msgstr "Настройка \"[cp_date_setting]\" не может быть больше текущей даты"

msgctxt "Languages::cp_discussion.enter_error_time"
msgid "Field \"Start date\" cannot be more or equal to \"End date\""
msgstr "Настройка \"Start date\" не может быть больше или равна \"End date\""

msgctxt "Languages::cp_discussion.run_manually"
msgid "Run manually"
msgstr "Запуск вручную"

msgctxt "Languages::cp_discussion.success_generate_time"
msgid "The dates of product reviews and discussions have been successfully updated"
msgstr "Даты отзывов и обсуждений товаров были успешно обновлены"